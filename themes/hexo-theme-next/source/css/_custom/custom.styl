// Custom styles.


#header {
    background:rgba(255,255,255,0.0) none repeat scroll !important;
}

body {   
    background:url(../images/mine.jpg);
    background-size:cover;
    background-repeat:no-repeat;
    background-attachment:fixed;
    background-position: center;
}

.content {
    margin: 5%;
    padding: 5%;
    background:rgba(255,255,255,0.35) none repeat scroll !important;
    -webkit-box-shadow: 0 0 14px rgba(202, 203, 203, .5);
    -moz-box-shadow: 0 0 14px rgba(202, 203, 204, .5);
}


.container .main-inner { width: 90%; }
.content-wrap {  width: 100%; }


#footer {
    background:rgba(255,255,255,0.0) none repeat scroll !important;
}



